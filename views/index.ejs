<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/app.css">
    <title>Document</title>
</head>
<body>
    <div class="author">
        <h1>Commits by Author</h1></div>
    <table id="users">
        <tr>
            <th>s.no</th>
            <th>Author Name</th>
            <th>Email</th>
            <th>Commits</th>
            <th>Verified</th>
        </tr>
    <% links.forEach(function(link,index){ %>
    <% if(isNaN(link.url.slice(-1))){ %>
        
        <tr class="yellow">
        <td><%= index+1 %></td>
        <td><%= link.name %></td>
        <td><%= link.email %></td>
        <td><a href="<%= link.url %>"><%= link.url %></a></td>
        <td><%= link.verified %></td>
        
        </tr>
    <% }else{ %>
        <tr class="red">
            <td><%= index+1 %></td>
            <td><%= link.name %></td>
            <td><%= link.email %></td>
            <td><a href="<%= link.url %>"><%= link.url %></a></td>
            <td><%= link.verified %></td>
            </tr>
    <% }}) %>      
    </table>
</body>
</html>